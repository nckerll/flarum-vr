{
    "name": "nckerll/flarum-vr",
    "description": "Flarum için bir WebXR arayüzü.",
    "type": "flarum-extension",
    "license": "MIT",
    "keywords": ["flarum", "vr", "webxr", "a-frame"],
    "require": {
        "flarum/core": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Nckerll\\FlarumVR\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Flarum VR",
            "category": "feature",
            "icon": {
                "name": "fas fa-vr-cardboard",
                "backgroundColor": "#6e44ff",
                "color": "#fff"
            }
        }
    }
}